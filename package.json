{
	"name": "gitdocs",
	"version": "2.0.0",
	"description": "Easy to use, SEO-friendly, beautiful documentation that lives in your git repo.",
	"license": "MIT",
	"homepage": "https://github.com/timberio/gitdocs#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timberio/gitdocs.git"
	},
	"engine": {
		"node": ">=6"
	},
	"keywords": [
		"documentation",
		"static",
		"markdown",
		"react",
		"docs"
	],
	"files": [
		"bin/",
		"dist/",
		"starter/",
		"license.md",
		"readme.md"
	],
	"main": "dist/api.js",
	"bin": {
		"gitdocs": "bin/gitdocs"
	},
	"scripts": {
		"test:lint": "eslint bin src tests",
		"test:unit": "ava 'src/**/*.spec.js'",
		"test:integration": "nyc ava 'tests/**/*.test.js'",
		"test:coverage": "nyc report -r=lcov",
		"test": "npm-run-all test:*",
		"coveralls": "cat coverage/lcov.info | coveralls",
		"precompile": "rm -rf dist",
		"compile": "babel src -d dist",
		"watch": "npm run compile -- -w",
		"build:macos": "pkg . -t node8.3-macos-x64 -o builds/gitdocs-macos-x64",
		"build:linux": "pkg . -t node8.3-linux-x64 -o builds/gitdocs-linux-x64",
		"build:windows": "pkg . -t node8.3-win-x64 -o builds/gitdocs-windows-x64",
		"prebuild": "npm run compile && rm -rf builds",
		"build": "npm-run-all build:*"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta.46",
		"@babel/core": "^7.0.0-beta.46",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
		"@babel/plugin-transform-runtime": "^7.0.0-beta.46",
		"@babel/preset-env": "^7.0.0-beta.46",
		"@babel/preset-react": "^7.0.0-beta.46",
		"@babel/register": "^7.0.0-beta.46",
		"ava": "^0.25.0",
		"coveralls": "^3.0.1",
		"eslint": "^4.19.1",
		"eslint-config-timber": "^1.0.19",
		"execa": "^0.10.0",
		"mock-fs": "^4.4.2",
		"npm-run-all": "^4.1.2",
		"nyc": "^11.7.1",
		"pkg": "4.2.4",
		"sinon": "^5.0.3"
	},
	"dependencies": {
		"@babel/runtime": "^7.0.0-beta.46",
		"axios": "^0.18.0",
		"babel-loader": "^8.0.0-beta.2",
		"chalk": "^2.4.1",
		"chokidar": "^2.0.3",
		"connect": "^3.6.6",
		"deepmerge": "^2.1.0",
		"exit-hook": "^2.0.0",
		"file-loader": "^1.1.11",
		"fs-extra": "^6.0.0",
		"glamor": "^2.20.40",
		"gray-matter": "^4.0.1",
		"html-minifier": "^3.5.15",
		"markdown-to-jsx": "^6.6.1",
		"minimist": "^1.2.0",
		"progress": "^2.0.0",
		"prop-types": "^15.6.1",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-helmet": "^5.2.0",
		"react-router-dom": "^4.2.2",
		"resolve": "^1.7.1",
		"webpack": "^4.6.0",
		"webpack-dev-middleware": "^3.1.3",
		"webpack-hot-middleware": "^2.22.1",
		"ws": "^5.1.1"
	},
	"pkg": {
		"scripts": "dist/**/*.js",
		"assets": [
			"starter/**/*"
		]
	},
	"eslintConfig": {
		"extends": "timber"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		],
		"plugins": [
			"@babel/plugin-transform-runtime",
			"@babel/plugin-proposal-object-rest-spread"
		],
		"ignore": [
			"**/*.{spec,test}.js"
		]
	},
	"ava": {
		"require": [
			"@babel/register"
		]
	},
	"nyc": {
		"all": true,
		"include": [
			"src"
		],
		"exclude": [
			"src/themes",
			"**/*.spec.js"
		]
	}
}
