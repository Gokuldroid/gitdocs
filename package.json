{
	"name": "gitdocs",
	"version": "2.0.0",
	"description": "Easy to use, SEO-friendly, beautiful documentation that lives in your git repo.",
	"license": "MIT",
	"homepage": "https://github.com/timberio/gitdocs#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timberio/gitdocs.git"
	},
	"engines": {
		"node": ">=8.6"
	},
	"keywords": [
		"documentation",
		"static",
		"markdown",
		"react",
		"docs"
	],
	"bin": {
		"gitdocs": "bin/gitdocs"
	},
	"scripts": {
		"test:lint": "eslint bin src tests themes",
		"test:unit": "ava 'src/**/*.spec.js'",
		"test:integration": "nyc ava 'tests/**/*.test.js'",
		"test:coverage": "nyc report -r=lcov",
		"test": "npm-run-all test:*",
		"coveralls": "cat coverage/lcov.info | coveralls",
		"build:macos": "pkg . -t node8.3-macos-x64 -o builds/gitdocs-macos-x64",
		"build:linux": "pkg . -t node8.3-linux-x64 -o builds/gitdocs-linux-x64",
		"build:windows": "pkg . -t node8.3-win-x64 -o builds/gitdocs-windows-x64",
		"build": "rm -rf builds && npm-run-all build:*"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"coveralls": "^3.0.1",
		"eslint": "^4.19.1",
		"eslint-config-timber": "^1.0.19",
		"execa": "^0.10.0",
		"mock-fs": "^4.5.0",
		"npm-run-all": "^4.1.3",
		"nyc": "^11.7.1",
		"pkg": "4.2.4",
		"sinon": "^5.0.7"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-dynamic-import-node": "^1.2.0",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-register": "^6.26.0",
		"chalk": "^2.4.1",
		"connect": "^3.6.6",
		"deepmerge": "^2.1.0",
		"exit-hook": "^2.0.0",
		"file-loader": "^1.1.11",
		"fs-extra": "^6.0.0",
		"glamor": "^2.20.40",
		"gray-matter": "^4.0.1",
		"html-minifier": "^3.5.15",
		"markdown-to-jsx": "^6.6.2",
		"minimist": "^1.2.0",
		"progress": "^2.0.0",
		"prop-types": "^15.6.1",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-helmet": "^5.2.0",
		"react-interface": "^0.9.7",
		"react-loadable": "^5.4.0",
		"react-router-dom": "^4.2.2",
		"react-smackdown": "1.0.0-beta.3",
		"react-syntax-highlighter": "^7.0.4",
		"webpack": "^4.8.1",
		"webpack-dev-middleware": "^3.1.3",
		"webpack-hot-middleware": "^2.22.1",
		"ws": "^5.1.1"
	},
	"pkg": {
		"scripts": [
			"src/**/*.js",
			"node_modules/webpack-hot-middleware/client.js"
		],
		"assets": [
			"starter/**/*",
			"themes/**/*"
		]
	},
	"eslintConfig": {
		"extends": "timber"
	},
	"nyc": {
		"all": true,
		"include": [
			"src"
		],
		"exclude": [
			"**/*.spec.js"
		]
	}
}
